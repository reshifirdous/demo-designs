    site__name: "{{ site_name }}"
    status__name: "Reserved"
    description: "WAN"
    tenant__name: "{{ tenant_name }}"

  - prefix: "{{ management_prefix }}"
    site__name: "{{ site_name }}"
    status__name: "Reserved"
    description: "Management"
    tenant__name: "{{ tenant_name }}"